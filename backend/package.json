    "mocha": "10.2.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "postgres": "^3.3.4",
    "sequelize-cli": "^6.6.0",
    "swagger-ui-express": "^4.6.2"
  }
}
